extends c
priority -50  

# if
snippet if
if (${1:condition}) {
	${2}
}
endsnippet

# fore
snippet fore
for (auto& ${1:item} : container) {
	
}
endsnippet

# switch
snippet switch
switch (${1:condition}) {
case :
	break;
default:
	break;
}
endsnippet


snippet mfun "member function" b
${1:void} ${2:className}::${3:FunctionName}(${4:param}) {
	${0:}
}
endsnippet


snippet fun "function" b
${1:ReturnType} ${2:FunctionName}(${3:param}) {
	${0:FunctionBody}
}
endsnippet
